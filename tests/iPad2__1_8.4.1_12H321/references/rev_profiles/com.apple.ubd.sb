			(regex #"^/private/var/mobile/Library/Logs/awd/awd-ubd[.]log" #"^/private/var/mobile/Library/processed-Mobile Documents[.]delete" #"^/private/var/mobile/Library/Mobile Documents" #"^/private/var/mobile/Library/Collections" #"^/private/var/mobile/Library/Logs/CrashReporter/DiagnosticLogs/ubiquity[.]log" #"^/private/var/mobile/Library/Logs/CrashReporter/DiagnosticLogs/ubiquity.+[.]log" #"^/private/var/mobile/Library/Logs/PersistentConnection/com[.]apple[.]ubd" #"^/private/var/mobile/Library/Logs/CrashReporter/PersistentConnection/com[.]apple[.]ubd" #"^/private/var/mobile/Library/Caches/com[.]apple[.]ubd/" #"^/private/var/mobile/Library/Caches/com[.]apple[.]ubd$" #"^/private/var/mobile/Library/Preferences/com[.]apple[.]ubd[.]plist" #"^/private/var/mobile/Library/Preferences/com[.]apple[.]mmcs[.]plist")
			(literal "/private/var/mobile/Library/Caches/.com.apple.persistentconnection.settings.lock.lock")
			(require-all
				(vnode-type REGULAR-FILE)
				(regex #"^/private/var/mobile/Library/Caches/com.apple.nsurlsessiond/Downloads/com[.]apple[.]ubd/" #"^/private/var/mobile/Library/Caches/com.apple.nsurlsessiond/Downloads/com[.]apple[.]ubd$"))
			(require-all
				(vnode-type DIRECTORY)
				(regex #"^/private/var/mobile/Library/Caches/com.apple.nsurlsessiond/Downloads/com[.]apple[.]ubd/" #"^/private/var/mobile/Library/Caches/com.apple.nsurlsessiond/Downloads/com[.]apple[.]ubd$"))
			(require-all
